<div class="row mt-2">
    <div class="col-lg-6 col-md-6 col-sm-12">
        <span class="cont-text"> Nama </span><br>
        <input  type="text" class="cont-form" name="namacust" id="cont" value="" placeholder="Masukan Nama Bussines Partner (PT,CV,TOKO, Dll)" ><br>
    </div>
    <div class="col-lg-3 col-md-3 col-sm-6">
        <span class="cont-text"> Phone </span><br>
        <input  type="text" class="cont-form" name="phonecust" id="cont" value="" placeholder="Masukan Nomor Telepon"><br>
    </div>
    <div class="col-lg-3 col-md-3 col-sm-6">
        <span class="cont-text"> Email </span><br>
        <input onblur="validasiEmailcp()" type="text" class="cont-form" name="emailcust" id="cont" value="" placeholder="Masukan Alamat Email" ><br>
    </div>
</div>
<div class="row mt-1">
    <div class="col-lg-6 col-md-6 col-sm-12">
        <span class="cont-text"> Lokasi Bussines Partner </span><br>
